{
  "name": "riptide-merchant",
  "version": "0.1.0",
  "author": "Marius Buleandra <mbuleandra@gmail.com>",
  "repository": "https://github.com/mariusbld/riptide-merchant",
  "scripts": {
    "clean": "shx rm -rf dist .parcel-cache",
    "build": "yarn clean && parcel build src/index.html",
    "start": "yarn clean && parcel src/index.html",
    "dev": "parcel serve src/index.html",
    "docs": "shx rm -rf docs && typedoc && shx cp .nojekyll docs/",
    "fmt": "prettier --write '{*,**/*}.{js,ts,jsx,tsx,json}'",
    "lint": "eslint . && prettier --check '{*,**/*}.{js,ts,jsx,tsx,json}'",
    "lint:fix": "eslint --fix . && yarn fmt",
    "nuke": "shx rm -rf node_modules yarn.lock"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.3.2",
    "@types/bootstrap": "^5.1.9",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.23",
    "parcel": "^2.3.2",
    "process": "^0.11.10"
  },
  "dependencies": {
    "@solana/pay": "^0.1.3",
    "@solana/wallet-adapter-base": "^0.9.3",
    "@solana/wallet-adapter-phantom": "^0.9.3",
    "@solana/wallet-adapter-react": "^0.15.3",
    "@solana/web3.js": "^1.35.1",
    "bignumber.js": "^9.0.2",
    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-date-picker": "^8.4.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.3"
  }
}
